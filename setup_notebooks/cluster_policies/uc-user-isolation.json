{
  "spark_version": {
    "type": "regex",
    "pattern": "1[0-1]\\.[0-9]*\\.x-scala.*",
    "defaultValue": "10.4.x-scala2.12"
  },
  "data_security_mode": {
    "type": "fixed",
    "value": "USER_ISOLATION",
    "hidden": true
  },
  "spark_conf.spark.databricks.unityCatalog.userIsolation.python.preview": {
    "type": "fixed",
    "value": "true"
  },
  "spark_conf.spark.databricks.dataLineage.enabled": {
    "type": "fixed",
    "value": "true"  
  }  
}